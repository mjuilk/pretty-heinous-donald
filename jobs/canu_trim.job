#!/bin/bash

#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=64
#SBATCH --mem=256G
#SBATCH --time=3-0
#SBATCH --output=canu_trim.%j.out
#SBATCH --error=canu_trim.%j.err
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=XXXXX@X.com

canu -trim -corrected -nanopore ~/data/X.fa -p X \
	-d ~/results/canu \
	genomeSize=3g \
	maxThreads=64 useGrid=false
